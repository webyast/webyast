msgstr "Következő"

#: app/views/registration/index.html.erb:131
msgid "Mandatory Information"
msgstr "Szükséges információ"

#: app/views/registration/index.html.erb:151
msgid "Details"
msgstr "Részletek"

#: app/views/registration/index.html.erb:179
msgid "Postpone"
msgstr "Elhalaszt"

