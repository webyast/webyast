# %s is either BOOTP or DHCP
#: app/controllers/time_controller.rb:117
msgid "Time settings have been written."
msgstr "​ការ​កំណត់​ពេលវេលា​​ត្រូវ​បាន​សរសេរ ។"

#: app/views/time/index.html.erb:54
msgid "Error: Cannot get information."
msgstr "កំហុស ៖ មិន​អាច​យក​ព័ត៌មាន​បាន​ទេ ។"

#: app/views/time/index.html.erb:79
msgid "Please wait"
msgstr "សូម​រង់ចាំ"

#: app/views/time/index.html.erb:86 app/views/time/index.html.erb:90
#: app/views/time/index.html.erb:111
msgid "This field is required"
msgstr "វាល​​នេះ​ត្រូវ​បាន​ទាមទារ"

#: app/views/time/index.html.erb:87 app/views/time/index.html.erb:112
msgid "Enter a valid domain name or an IP address"
msgstr "បញ្ចូល​ឈ្មោះ​ដែន​ ឬ​អាសយដ្ឋាន IP ដែល​ត្រឹមត្រូវ"

#: app/views/time/index.html.erb:91
msgid "Invalid time format. Correct one is hh:mm:ss"
msgstr "ទ្រង់ទ្រាយ​ពេលវេលា​មិន​ត្រឹមត្រូវ ។ ទ្រង់ទ្រាយ​ដែល​ត្រឹមត្រូវ​គឺ hh:mm:ss"

#: app/views/time/index.html.erb:154 app/views/time/index.html.erb:198
#: app/views/time/index.html.erb:216
msgid "Time"
msgstr "ពេលវេលា"

#: app/views/time/index.html.erb:167 app/views/time/index.html.erb:177
msgid "Timezone"
msgstr "តំបន់​ពេលវេលា"

#: app/views/time/index.html.erb:172
msgid "Region"
msgstr "តំបន់"

#: app/views/time/index.html.erb:186
msgid "Hardware clock is set to UTC"
msgstr "នាឡិកា​ផ្នែក​រឹង​ត្រូវ​បាន​កំណត់​ទៅ UTC"

# 55493 AttribValues/label
#: app/views/time/index.html.erb:207
msgid "Manually configure time"
msgstr "កំណត់​រចនាសម្ព័ន្ធ​ពេលវេលា​ដោយដៃ"

#: app/views/time/index.html.erb:211
msgid "Date"
msgstr "កាល​បរិច្ឆេទ"

#: app/views/time/index.html.erb:226
msgid "Synchronize via Network Time Protocol (NTP)"
msgstr "ធ្វើ​សមកាលកម្ម​តាមរយៈ Network Time Protocol (NTP)"

#: app/views/time/index.html.erb:231
msgid "NTP server"
msgstr "ម៉ាស៊ីន​បម្រើ NTP"
